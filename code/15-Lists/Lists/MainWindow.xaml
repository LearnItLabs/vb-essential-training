<Window x:Class="MainWindow"
				xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
				xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
				xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
				xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lists"
        mc:Ignorable="d"
				Title="Tabbed Explorer"
				FontSize='24'
				Height="600"
				Width="1000"
				WindowStartupLocation='CenterScreen'
				WindowState='Maximized'>


		<TabControl Background='LightSteelBlue'
								Grid.Column='1'>
			<TabItem Header='Create and Initialize'>
				<local:CreateDemo />
			</TabItem>
			<TabItem Header='Add'>
				<local:AddDemo />
			</TabItem>
		<TabItem Header='Iterate'>
			<local:IterateDemo />
		</TabItem>
		<TabItem Header='Predicate'>
			<local:PredicateDemo />
		</TabItem>
		
			<TabItem Header='Properties'>
				<local:PropertiesDemo />
			</TabItem>
			<TabItem Header='Extension Methods'>
				<local:ExtensionDemo />
			</TabItem>
			<TabItem Header='LINQ'>
				<local:LinqDemo />
			</TabItem>
			<TabItem Header='Other'>
				<local:OtherDemo />
			</TabItem>
		<TabItem Header='UI Listbox'>
			<local:UIDemo />
		</TabItem>

	</TabControl >
	
	
</Window>

